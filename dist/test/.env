# Give your application a unique name (no spaces)
APP_NAME=test
APP_URL=http://localhost:4000

# false for production, true for development
DEBUG=true

# the port should we listen on
PORT=4000

# the server name, e.g, www.mysite.com
SERVER_NAME=localhost

# should we use https?
SECURE=false

# database config - postgres or mysql
DATABASE_TYPE=mysql
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_USER=mariadb
DATABASE_PASS=password
DATABASE_NAME=microGo
DATABASE_SSL_MODE=true
DATABASE_TIME_ZONE=Asia/Shanghai

# redis config
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_PREFIX=test

# cache (Supported cache databases redis/badger)
CACHE=badger

# cooking settings
COOKIE_NAME=test
COOKIE_LIFETIME=1
COOKIE_PERSIST=true
COOKIE_SECURE=false
COOKIE_DOMAIN=localhost

# session store: cookie, redis, mysql, or postgres
SESSION_TYPE=cookie

# mail settings
SMTP_HOST=
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_PORT=25
SMTP_ENCRYPTION=none
FROM_NAME=
FROM_ADDRESS=
MAIL_DOMAIN=

# mail settings for api services
MAILER_API=
MAILER_KEY=
MAILER_URL=

# template engine: go/blocks/jet
RENDERER=blocks

# the encryption key; must be exactly 32 characters long
ENCRYPTION_KEY=4ff4D44d4s5fsw6D64D4Df4f47d44fw5